{
  "version": 2,
  "builds": [
    {
      "src": "campus_shop/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    },
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/campus_shop/wsgi.py"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/campus_shop/wsgi.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ],
  "functions": {
    "campus_shop/wsgi.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "PYTHONPATH": ".",
    "DJANGO_SETTINGS_MODULE": "campus_shop.settings"
  }
}